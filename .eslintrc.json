{
  "env": {
    "node": true,
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier", "eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "indent": "warn",
    "no-plusplus": "warn",
    "import/extensions": "warn",
    "object-curly-newline": "warn",
    "object-curly-spacing": "warn",
    "linebreak-style": "warn"
  },
  "globals": {
    "Handlebars": true
  }
}
